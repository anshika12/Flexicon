{
    "conversions": {
        "fahrenheit_to_kelvin": {
            "formula": "(value - 32) * (5/9) + 273.15",
            "units": ["fahrenheit", "kelvin"]
        },
        "fahrenheit_to_celsius": {
            "formula": "(value - 32) * (5/9)",
            "units": ["fahrenheit", "celsius"]
        },
        "fahrenheit_to_rankine": {
            "formula": "value + 459.67",
            "units": ["fahrenheit", "rankine"]
        },
        "kelvin_to_fahrenheit": {
            "formula": "value * (9/5) - 459.67",
            "units": ["kelvin", "fahrenheit"]
        },
        "kelvin_to_celsius": {
            "formula": "value - 273.15",
            "units": ["kelvin", "celsius"]
        },
        "kelvin_to_rankine": {
            "formula": "value * (9/5)",
            "units": ["kelvin", "rankine"]
        }
    }
}